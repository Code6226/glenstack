{
  "name": "@glenstack/ui",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "files": [
    "./dist/"
  ],
  "scripts": {
    "postinstall": "npm run build:lib",
    "build": "npm run build:lib && npm run build:storybook",
    "build:lib": "tsc --build tsconfig.build.json",
    "build:storybook": "build-storybook",
    "start": "run-p start:storybook start:expo",
    "start:storybook": "start-storybook -p 6006",
    "start:expo": "expo start --dev",
    "eject": "expo eject",
    "test": "jest --coverage=false --changedSince=origin/master",
    "test:ci": "jest",
    "clean": "rimraf ./.expo ./storybook-static",
    "deploy": "wrangler publish"
  },
  "dependencies": {
    "@expo/html-elements": "0.0.0-alpha.5",
    "css": "file:./src/base/css",
    "expo": "38.0.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.0.tar.gz",
    "react-native-safe-area-context": "3.0.7",
    "react-native-screens": "2.9.0",
    "react-native-web": "0.13.0",
    "react-native-web-hooks": "3.0.1",
    "react-responsive": "8.1.0",
    "tailwind-rn": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@cloudflare/wrangler": "1.10.3",
    "@expo/webpack-config": "0.12.17",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addon-ondevice-actions": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/react": "5.3.19",
    "@storybook/react-native": "5.3.19",
    "@types/jest": "26.0.3",
    "@types/react": "16.9.41",
    "@types/react-native": "0.62.14",
    "@types/react-responsive": "8.0.2",
    "@types/react-test-renderer": "16.9.2",
    "babel-loader": "8.1.0",
    "babel-preset-expo": "8.2.3",
    "expo-cli": "3.21.13",
    "expo-module-scripts": "1.2.0",
    "jest": "26.1.0",
    "npm-run-all": "4.1.5",
    "react-native-testing-library": "2.1.0",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "typescript": "3.9.5"
  }
}
