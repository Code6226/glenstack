{
  "name": "@glenstack/ui",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "files": [
    "./dist/"
  ],
  "scripts": {
    "postinstall": "npm run build",
    "build": "npm run build:lib && npm run build:storybook",
    "build:lib": "tsc --build tsconfig.build.json",
    "build:storybook": "build-storybook",
    "start": "run-p start:storybook start:expo",
    "start:storybook": "start-storybook -p 6006",
    "start:expo": "expo start --dev",
    "eject": "expo eject",
    "test": "jest --coverage=false --changedSince=origin/master",
    "test:ci": "jest",
    "clean": "rimraf ./.expo ./storybook-static",
    "deploy": "wrangler publish"
  },
  "dependencies": {
    "expo": "37.0.10",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "~2.2.0",
    "react-native-web": "0.12.2"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@cloudflare/wrangler": "1.9.0",
    "@expo/webpack-config": "0.12.6",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-links": "5.3.18",
    "@storybook/addon-ondevice-actions": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/react": "5.3.18",
    "@storybook/react-native": "5.3.18",
    "@types/jest": "25.2.2",
    "@types/react": "16.9.35",
    "@types/react-native": "0.62.8",
    "@types/react-test-renderer": "16.9.2",
    "babel-loader": "8.1.0",
    "babel-preset-expo": "8.1.0",
    "expo-cli": "3.20.3",
    "expo-module-scripts": "1.2.0",
    "jest": "26.0.1",
    "npm-run-all": "4.1.5",
    "react-native-testing-library": "1.13.2",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "typescript": "3.9.2"
  }
}
