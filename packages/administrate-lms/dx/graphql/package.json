{
  "name": "graphql",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run download:lms",
    "download:lms": "node downloadSchema.js",
    "prevoyager": "npm run download:lms",
    "voyager": "node voyager/index.js",
    "preupdate:lms": "npm run download:lms",
    "update:lms": "apollo client:codegen --target=typescript --tsFileExtension='d.ts' --customScalarsPrefix=ADM --globalTypesFile='../../src/__generated__/globalTypes.d.ts'"
  },
  "dependencies": {
    "cross-fetch": "3.0.5",
    "express": "4.17.1",
    "graphql-voyager": "1.0.0-rc.31",
    "openurl": "1.1.1"
  },
  "devDependencies": {
    "apollo": "2.28.3"
  }
}
